  "license": "ISC",
  "dependencies": {
    "dotenv": "^6.0.0",
    "pg": "^7.4.3",
    "morgan": "^1.9.1",
    "express": "^4.16.3"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "nodemon": "^1.18.4"
  }
